{
  "name": "broid-google-assistant",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "AGPL-3.0+",
  "author": "Broid Team <opensource@broid.ai> (https://broid.ai)",
  "description": "Convert Google Assistant messages into Activity Streams 2 with Broid Integration",
  "repository": {
    "type": "git",
    "url": "git@github.com:broidHQ/integrations.git"
  },
  "bugs": {
    "url": "https://github.com/broidHQ/integrations/issues"
  },
  "homepage": "https://github.com/broidHQ/integrations/tree/master/broid-google-assistant#readme",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run build && nyc ava --verbose",
    "build": "npm run clean && npm run copy && tsc",
    "clean": "rm -rf lib",
    "copy": "copyfiles -u 1 src/**/*.json lib/",
    "watch": "concurrently --kill-others \"npm run lint:watch\" \"npm run tsc:watch\"",
    "lint": "tslint -c tslint.json src/*",
    "lint:watch": "watch 'tslint -c tslint.json src/*' src",
    "tsc:watch": "tsc --watch",
    "travis": "npm run lint && npm run build && npm run test",
    "release": "npm run travis && npm run build && npm publish"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/node": "^7.0.5",
    "@types/node-uuid": "0.0.28",
    "@types/ramda": "0.0.3",
    "ava": "^0.18.1",
    "concurrently": "^3.1.0",
    "copyfiles": "^1.2.0",
    "nyc": "^10.1.2",
    "sinon": "^1.17.7",
    "tslint": "^4.3.1",
    "tslint-eslint-rules": "^3.2.3",
    "typescript": "^2.1.4",
    "watch": "^1.0.1"
  },
  "dependencies": {
    "actions-on-google": "^1.0.5",
    "bluebird": "^3.4.7",
    "body-parser": "^1.17.1",
    "broid-schemas": "^1.1.4",
    "broid-utils": "^1.0.4",
    "express": "^4.14.0",
    "node-uuid": "^1.4.7",
    "ramda": "^0.23.0",
    "rxjs": "^5.0.2"
  },
  "ava": {
    "files": [
      "lib/test/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true
  },
  "nyc": {
    "exclude": [
      "dist"
    ]
  }
}
